<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ClocKeys"
        tools:targetApi="31">
        <activity
            android:name=".Startups.UserSignup.SignupBioActivity"
            android:exported="false" />
        <activity
            android:name=".Startups.UserSignup.SignupAddressActivity"
            android:exported="false" />
        <activity
            android:name=".Startups.UserSignup.SignupDateOfBirthActivity"
            android:exported="false" />
        <activity
            android:name=".Startups.UserSignup.SignupNameActivity"
            android:exported="true" />
        <activity
            android:name=".Management.ProfileInputActivity"
            android:exported="false" />
        <activity
            android:name=".Management.CameraActivity"
            android:exported="false" />
        <activity
            android:name=".Management.EditProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Management.TimecardActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Startups.LoginStart"
            android:exported="true"
            android:label="Login" />
    </application>

</manifest>